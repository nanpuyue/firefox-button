{
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "minimize-window-button@nanpuyue.com",
			"strict_min_version": "57.0a1"
		}
	},
	"name": "__MSG_extensionName__",
	"version": "1.0.2",
	"description": "__MSG_extensionDescription__",
	"icons": {
		"48": "icons/icon.png"
	},
	"browser_action": {
		"browser_style": false,
		"default_title": "__MSG_actionTitle__",
